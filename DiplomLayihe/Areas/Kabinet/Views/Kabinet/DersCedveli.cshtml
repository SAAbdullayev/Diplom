@{ 
    DateTime timeNow = DateTime.UtcNow.AddHours(4);


}

<div class="content">
    <table class="table table-hover cedvel">
        <thead>
            <tr>
                <th scope="col">Monday</th>
                <th scope="col">Tuseday</th>
                <th scope="col">Wednesday</th>
                <th scope="col">Thursday</th>
                <th scope="col">Friday</th>
            </tr>
        </thead>
        <tbody>
