                    <tr>
                        <td>
                            @if (item.BashlamaVaxti.DayOfWeek.ToString() == "Monday")
                            {
                                <p>Bashlayir: @item.BashlamaVaxti.ToString("HH:mm")</p>
                                <p>Bitir: @item.BitmeVaxti.ToString("HH:mm")</p>
                                if (User.IsInRole("Muellim"))
                                {

                                    @foreach (var qrup in ViewBag.Groups)
                                    {
                                        if (qrup.Id == item.GroupId)
                                        {
                                            <p> Qrup: @qrup.Name</p>
                                        }
                                    }

                                    if (item.BashlamaVaxti < timeNow && timeNow < item.BitmeVaxti && User.IsInRole("Muellim"))
                                    {
                                        <a class="btn btn-success" asp-route-id="@item.Id" asp-action="qiymetlendirme">Daxil ol</a>
                                    }
                                }
                                else if (User.IsInRole("Telebe"))
                                {
                                    @foreach (var fenn in ViewBag.Fenn)
                                    {
                                        if (fenn.Id == item.FennId)
                                        {
                                            <p>Fenn:@fenn.Name</p>

