@model SigninCommand


<div class="signin">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="signinheading">
                    <h2>Sign In</h2>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-3 col-md-3"></div>
            <div class="col-lg-6 col-md-6 col-sm-12">
                <form method="post" asp-action="signin" class="signinform">
                    <div class="inputarea">
                        <label for="username">Username</label>
                        <input asp-for="Username" type="text" name="username" id="">
                    </div>
                    <div class="inputarea">
                        <label for="password">Password</label>
                        <input asp-for="Password" type="password" name="password" id="">
                    </div>
                    <button>Sign In</button>
                </form>
            </div>
            <div class="col-lg-3 col-md-3"></div>
        </div>
    </div>
</div>